{
  "name": "vue-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run lint --fix",
    "dev": "vue-cli-service serve",
    "//dev-test": "本地代码测试 个人电脑",
    "dev-test": "vue-cli-service build --mode test && node server.js",
    "serve": "vue-cli-service serve",
    "//build-staging": "打包本地代码 一般用与本地测试 内部服务器",
    "build-staging": "vue-cli-service build --mode staging",
    "//prebuild": "线上测试是更新版本号 代码必须提交",
    "prebuild": "npm version patch",
    "//build": "打包本地代码 一般用与线上测试",
    "build": "vue-cli-service build",
    "//build-release": "打包代码 进行发版上线",
    "build-release": "vue-cli-service build --mode release",
    "server": "cross-env NODE_ENV=production node server.js",
    "lint": "vue-cli-service lint",
    "report": "vue-cli-service build --report",
    "new": "node ./script/generateView",
    "commit": "npx git-cz",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "ant-design-vue": "^1.3.9",
    "axios": "^0.18.0",
    "core-js": "^2.6.5",
    "deepmerge": "^3.2.0",
    "iview": "^3.4.2",
    "js-cookie": "^2.2.0",
    "moment": "^2.24.0",
    "qs": "^6.7.0",
    "uuid": "^3.3.2",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "@sentry/webpack-plugin": "^1.6.2",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-e2e-nightwatch": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "commitizen": "^3.1.1",
    "compression-webpack-plugin": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "raw-loader": "^2.0.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "vue-skeleton-webpack-plugin": "^1.2.2",
    "vue-template-compiler": "^2.5.21",
    "vuepress": "^1.0.0-alpha.48",
    "zip-webpack-plugin": "^3.0.0"
  }
}
